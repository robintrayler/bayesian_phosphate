data <- read_csv(file = './data/data.csv') %>% 
  filter(group == 'pliocene_north_carolina' & thermo %in% c('Meso', 'Ecto')) %>% 
  select(thermo, 
         genus, 
         species,
         d18Op) %>% 
  group_by(genus, species) %>% 
  summarize(mean = mean(d18Op),
            sd = sd(d18Op),
            thermo = unique(thermo),
            n = n())
